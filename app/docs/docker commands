
# APP
docker build -t app_image -f deploy/Dockerfile .
docker run --name app_container -d -p 8000:8000 app_image
docker logs app_container

# REMOVE 
docker kill $(docker ps -q)
docker rm -f $(docker ps -a -q)
yes | docker image prune -a

# BEST COMMAND EVER
kubectl get pods -n jonhcena
kubectl -n jonhcena port-forward app-5cdb478f89-l88nb 1232:8000
http://localhost:1232/
